// The Process: Light-Independent Reactions (Calvin Cycle): Carbon dioxide is fixed. Sugar is produced.
digraph {
	rankdir=TB
	node [fillcolor=lightyellow shape=ellipse style=filled]
	edge [color=blue dir=forward]
	step_0 [label="Step 1"]
	step_1 [label="Step 2"]
	step_0 -> step_1
	step_2 [label="Step 3"]
	step_1 -> step_2
	step_3 [label="Step 4"]
	step_2 -> step_3
	step_3 -> step_0 [style=dashed]
}
